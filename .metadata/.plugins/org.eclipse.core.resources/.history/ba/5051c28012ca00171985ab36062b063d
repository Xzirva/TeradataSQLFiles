-- DROP TABLE prdwa17_staging.channel; CREATE TABLE prdwa17_staging.channels ( 	id VARCHAR(50), 	title VARCHAR(255), 	description TEXT, 	publishedAt VARCHAR(255), 	viewCount BIGINT, 	commentCount BIGINT,	subscriberCount BIGINT,	videoCount BIGINT,	topicCategory_1 VARCHAR(100),	topicCategory_2 VARCHAR(100),	topicCategory_3 VARCHAR(100),	keywords VARCHAR(100),	PRIMARY KEY(id) );  CREATE TABLE videos ( 	dbId SERIAL,  	id VARCHAR(50),  	channelId VARCHAR(50), 	title VARCHAR(255), 	description TEXT, 	publishedAt VARCHAR(255), 	viewCount BIGINT, 	commentCount BIGINT, 	likeCount BIGINT, 	dislikeCount BIGINT, 	favoriteCount BIGINT,	subscriberCount BIGINT,	videoCount BIGINT,	categoryId VARCHAR(50),	topicCategory_1 VARCHAR(100),	topicCategory_2 VARCHAR(100),	topicCategory_3 VARCHAR(100),	keywords VARCHAR(100),	PRIMARY KEY(dbId) );  CREATE TABLE YoutubeTags ( 	title VARCHAR(50), 	videoId INT, 	FOREIGN KEY(videoId) REFERENCES VIDEOS.dbId, 	PRIMARY KEY (title, videoId) );