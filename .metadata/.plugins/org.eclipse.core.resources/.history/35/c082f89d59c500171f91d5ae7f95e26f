--CREATE SCHEMA prdwa17_target;
--CREATE SCHEMA prdwa17_ml;
--DROP TABLE tmp_content;

CREATE FACT TABLE prdwa17_staging.tmp_content
(
   id integer NOT NULL,
   content character varying,
   CONSTRAINT tmp_content_pkey PRIMARY KEY (id)
)
DISTRIBUTE BY HASH (id)
STORAGE ROW;

CREATE INDEX tmp_content_pkey
   ON prdwa17.tmp_content(id ASC NULLS LAST);