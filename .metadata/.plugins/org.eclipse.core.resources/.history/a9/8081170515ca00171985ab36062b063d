--DROP TABLE prdwa17_staging.channels; CREATE TABLE prdwa17_staging.channels ( 	dbId BIGSERIAL LOCAL, 	id VARCHAR(50), 	title VARCHAR(255), 	description TEXT, 	publishedAt VARCHAR(255), 	viewCount BIGINT, 	commentCount BIGINT,	subscriberCount BIGINT,	videoCount BIGINT,	topicCategory_1 VARCHAR(100),	topicCategory_2 VARCHAR(100),	topicCategory_3 VARCHAR(100),	keywords VARCHAR(100),	PRIMARY KEY(dbId) );  CREATE TABLE prdwa17_staging.videos ( 	dbId BIGSERIAL LOCAL,  	id VARCHAR(50),  	channelId VARCHAR(50), 	title VARCHAR(255), 	description TEXT, 	publishedAt VARCHAR(255), 	viewCount BIGINT, 	commentCount BIGINT, 	likeCount BIGINT, 	dislikeCount BIGINT, 	favoriteCount BIGINT,	subscriberCount BIGINT,	videoCount BIGINT,	categoryId VARCHAR(50),	topicCategory_1 VARCHAR(100),	topicCategory_2 VARCHAR(100),	topicCategory_3 VARCHAR(100),	keywords VARCHAR(100),	PRIMARY KEY(dbId) );  CREATE TABLE prdwa17_staging.YouTubeTags ( 	title VARCHAR(50), 	videoId BIGINT, 	FOREIGN KEY(videoId) REFERENCES videos(dbId), 	PRIMARY KEY (title, videoId) );  CREATE TABLE prdwa17_staging.videosComments ( 	dbId BIGSERIAL LOCAL PRIMARY KEY, 	authorChannelUrl VARCHAR(50), 	authorChannelId VARCHAR(50), 	videoId BIGINT REFERENCES videos(dbId) );/* CREATE TABLE channelComments ( 	dbId BIGSERIAL PRIMARY KEY, 	authorChannelUrl VARCHAR(50), 	authorChannelId VARCHAR(50), 	channelId BIGINT REFERENCES videos(dbId) );*/