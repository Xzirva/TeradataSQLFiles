--CREATE SCHEMA prdwa17_ml;
CREATE TABLE tmp_content(
	id int NOT NULL AUTO_INCREMENT,
	content TEXT,
	PRIMARY KEY(id)
)

CREATE TABLE Persons (
    ID INTEGER GENERATED ALWAYS AS IDENTITY
(START WITH 1 
INCREMENT BY 1 ),
    LastName varchar(255) NOT NULL,
    FirstName varchar(255),
    Age int,
    PRIMARY KEY (ID)
);